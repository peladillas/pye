.sub_title_prod_desk.prim{padding-top:3vh;}
.sub_title_prod_desk{color:black;font-size:1.5vw;}
.img-prod_desk{margin:0px auto;width:40%;}
.img-footer{margin:5% auto;width:50%;}
.pantalla{height:100%;}
.social_footer{margin:0px auto;}
.firma{margin-top:2vh;}
.divisor{color:black;margin:0px 0px 10px 0px;border-color:black;}
.social{border:2px solid black;}
.cont_social{min-height:10vh;padding:15px;}
.pye_footer_land{display:none;}
.pye_footer_productos_land{display:none;}
}
