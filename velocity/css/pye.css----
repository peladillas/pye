.img-prod{margin:1px auto;width:30%;}
.img-footer.footer_img_land{margin:0% auto;width:20%;}
.pantalla{height:100%;}
.social_footer{margin:0px auto;}
.firma{margin-top:2vh;}
.divisor{color:black;margin:10px 0px 10px 0px;border-color:black;}
.social{border:2px solid black;}
.cont_social{min-height:10vh;padding:15px;}
.pye_footer{display:none;}
.logo_chat{cursor:pointer;width:8vw;}
.cont_desk_logo{}
.desk_logo{margin:0px auto;height:10vh;}
.pye_footer_land p{height:30%;padding-top:10vh;}
.pye_footer_productos{display:none;}
.pye_footer_productos_land{margin-top:3vw;}
}
