.section{height: 100vh;}
@font-face{font-family:'Kelson Sans';src:url('../font/KelsonSans-Light.eot');src:local('../font/Kelson Sans Light'), local('KelsonSans-Light'),
url('../font/KelsonSans-Light.eot?#iefix') format('embedded-opentype'),
url('../font/KelsonSans-Light.woff2') format('woff2'),
url('../font/KelsonSans-Light.woff') format('woff'),
url('../font/KelsonSans-Light.ttf') format('truetype'),
url('../font/KelsonSans-Light.svg#KelsonSans-Light') format('svg');font-weight:300;font-style:normal;}
@font-face{font-family:'Kelson Sans Bold';src:url('../font/KelsonSans-Bold.eot');src:local('../font/Kelson Sans Bold'), local('KelsonSans-Bold'),
url('../font/KelsonSans-Bold.eot?#iefix') format('embedded-opentype'),
url('../font/KelsonSans-Bold.woff2') format('woff2'),
url('../font/KelsonSans-Bold.woff') format('woff'),
url('../font/KelsonSans-Bold.ttf') format('truetype'),
url('../font/KelsonSans-Bold.svg#KelsonSans-Bold') format('svg');font-weight:bold;font-style:normal;font-weight:300;}
div.mensajes{margin-top:2vh;}
.newsletter,.link_pye{cursor:pointer;}
@media screen and (orientation:portrait){.land{display:none;}
.table-bordered tr, .table-bordered td{border-color:black !important;}
.full_pye{padding:0px;height:100vh;}
.bg-primera{background:#000000;}
.bg-segunda{background:#FFFFFF;}
.bg-footer{background:#FFFF33;}
.pad15{padding:15px;}
.pye_head{height:30%;padding-top:5vh;}
.pye_body{height:30%;}
.pye_footer{height:40%;}
.pye_head_productos{height:25%;}
.pye_body_productos{height:35%;}
.pye_body_productos_titulo{min-height:15%;margin-bottom:5%;}
.pye_body_productos_img{max-height:10%;}
.pye_footer_productos{height:20%;margin-top:5%;}
.pye_body_fin{height:25%;}
.pye_head_footer{height:15%;}
.pye_footer_productos p{margin:0px;}
.kregular{color:white;font-family:'Kelson Sans Bold';}
.klight{color:black;font-family:'Kelson Sans';}
.text_pye_contacto{margin-right:3vw;font-size:5vw;}
.text_pye_portada{font-size:8vw;}
.text_pye_prod_title{font-size:7vw;color:black;}
.sub_text_pye_prod_title{font-size:6vw;color:black;}
.copy{font-size:4vw;color:black;}
.text_pye_prod_title_final{font-size:6vw;color:black;}
.pye_body_fin_img{}
.bg-img-prod{width:60vw;height:60vw;background-color:#FFFF33;margin-left:20vw;}
#logo_portada{margin:auto;width:100%;}
.img-prod{margin:37% auto;width:50%;}
.img-footer{margin:5% auto;width:50%;}
.pantalla{height:100%;}
.social_footer{margin:0px auto;}
.firma{margin-top:2vh;}
.divisor{color:black;margin:0px 0px 10px 0px;border-color:black;}
.social{border:2px solid black;}
.cont_social{min-height:10vh;padding:15px;}
.pye_footer_land{display:none;}
.pye_footer_productos_land{display:none;}
}
